<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
               <children>
                  <TextField id="nameTextField" promptText="Player Name" />
                  <ColorPicker id="colorPicker" prefHeight="26.0" prefWidth="163.0" promptText="Player Color" />
                  <ComboBox id="raceComboBox" prefHeight="26.0" prefWidth="163.0" promptText="Player Race" />
               </children>
            </VBox>
         </center>
         <bottom>
            <Button id="doneButton" mnemonicParsing="false" text="Done" BorderPane.alignment="CENTER" />
         </bottom>
         <top>
            <HBox prefHeight="45.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</VBox>
